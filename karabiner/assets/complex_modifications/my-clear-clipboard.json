{
  "title": "Clear Clipboard by Fn Fn",
  "rules": [
    {
      "description": "Clear Clipboard by Fn Fn",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "fn"
              },
              {
                "key_code": "fn"
              }
            ]
          },
          "to_if_alone": [
            {
              "shell_command": "/usr/bin/env cat | /usr/bin/env pbcopy"
            }
          ]
        }
      ]
    }
  ]
}